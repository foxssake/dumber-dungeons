{
  "name": "dumber-dungeons",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write \"packages/*src/**/*.ts\" \"packages/*/test/**/*.ts\"",
    "format:check": "prettier \"packages/*/src/**/*.ts\" \"packages/*/test/**/*.ts\"",
    "lint": "eslint \"packages/*/{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:inspect": "eslint --inspect-config"
  }
  "workspaces": [
    "packages/*"
  ]
}
