{
  "name": "@dumber-dungeons/client",
  "type": "module",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "build": "bun scripts/build.ts",
    "build:dev": "bun scripts/bundle.ts --watch",
    "serve": "bunx http-server",
    "start": "concurrently \"bun run build\" \"bun run serve\"",
    "start:dev": "concurrently \"bun run build:dev\" \"bun run serve\"",
    "test": "bun test src",
    "test:watch": "bun test src --watch",
    "test:cov": "bun test src --coverage",
    "test:debug": "bun test src --inspect-brk",
    "test:e2e": "bun test test/e2e",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "format:check": "prettier \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:inspect": "eslint --inspect-config"
  },
  "dependencies": {
    "react": "19.0.0-rc.1",
    "react-dom": "19.0.0-rc.1",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/three": "^0.170.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.0",
    "prettier": "3.3.3",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  }
}
